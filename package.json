{
  "name": "graphqlade",
  "version": "0.3.3",
  "description": "A complete library for GraphQL + TypeScript development on server- and client-side",
  "keywords": [
    "graphql",
    "typescript",
    "code-generation",
    "server",
    "client",
    "websockets"
  ],
  "homepage": "https://github.com/morris/graphqlade",
  "repository": {
    "type": "git",
    "url": "https://github.com/morris/graphqlade.git"
  },
  "bugs": {
    "url": "https://github.com/morris/graphqlade/issues"
  },
  "contributors": [
    "Morris Brodersen <mb@morrisbrodersen.de> (https://morrisbrodersen.de)"
  ],
  "license": "ISC",
  "engines": {
    "node": ">=12"
  },
  "module": "src/index.ts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bash scripts/build.sh",
    "build-examples": "bash scripts/build-examples.sh",
    "clean": "bash scripts/clean.sh",
    "example": "bash scripts/example.sh",
    "format": "prettier --write './**/*.ts' './**/*.js' './**/*.json'",
    "format-check": "prettier --check './**/*.ts' './**/*.js' './**/*.json'",
    "lint": "eslint .",
    "maybe-publish": "bash scripts/maybe-publish.sh",
    "pipeline": "bash scripts/pipeline.sh",
    "test": "nyc mocha test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run build && npm run test"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint"
    ]
  },
  "peerDependencies": {
    "graphql": "^15",
    "prettier": "^2",
    "typescript": "^4",
    "ws": "^7"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/got": "^9.6.12",
    "@types/graphql-iso-date": "^3",
    "@types/mocha": "^9.0.0",
    "@types/node": "^14.17.12",
    "@types/prettier": "^2.3.2",
    "@types/ws": "^7.4.7",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "express": "^4",
    "got": "^11.8.2",
    "graphql": "^15.5.1",
    "graphql-iso-date": "^3.6.1",
    "husky": "^4.3.8",
    "lint-staged": "^11.1.2",
    "mocha": "^9.1.1",
    "nyc": "^15",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5",
    "terser": "^5.7.2",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.2",
    "ws": "^7.5.4"
  }
}
