{
  "name": "graphqlade",
  "version": "0.3.1",
  "description": "A complete library for GraphQL + TypeScript development on server- and client-side",
  "keywords": [
    "graphql",
    "typescript",
    "code-generation",
    "server",
    "client",
    "websockets"
  ],
  "homepage": "https://github.com/morris/graphqlade",
  "repository": {
    "type": "git",
    "url": "https://github.com/morris/graphqlade.git"
  },
  "bugs": {
    "url": "https://github.com/morris/graphqlade/issues"
  },
  "contributors": [
    "Morris Brodersen <mb@morrisbrodersen.de> (https://morrisbrodersen.de)"
  ],
  "license": "ISC",
  "engines": {
    "node": ">=12"
  },
  "module": "src/index.ts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bash scripts/build.sh",
    "build-examples": "bash scripts/build-examples.sh",
    "clean": "bash scripts/clean.sh",
    "example": "bash scripts/example.sh",
    "format": "prettier --write './**/*.ts' './**/*.js' './**/*.json'",
    "lint": "eslint .",
    "maybe-publish": "bash scripts/maybe-publish.sh",
    "test": "nyc mocha test"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "src"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "html"
    ],
    "cache": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run build && npm run test"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint"
    ]
  },
  "peerDependencies": {
    "graphql": "^15",
    "prettier": "^2",
    "typescript": "^4",
    "ws": "^7"
  },
  "devDependencies": {
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.11",
    "@types/got": "^9",
    "@types/graphql-iso-date": "^3",
    "@types/mocha": "^8",
    "@types/node": "^14.14.20",
    "@types/prettier": "^2.1.6",
    "@types/ws": "^7.4.0",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "eslint": "^7.17.0",
    "express": "^4",
    "got": "^11.8.1",
    "graphql": "^15",
    "graphql-iso-date": "^3.6.1",
    "husky": "^4.3.7",
    "lint-staged": "^10",
    "mocha": "^8",
    "nyc": "^15",
    "prettier": "^2",
    "source-map-support": "^0.5",
    "terser": "^5.5.1",
    "ts-node": "^9.1.1",
    "typescript": "^4",
    "ws": "^7.4.2"
  }
}
