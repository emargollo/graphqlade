{
  "name": "graphqlade",
  "version": "0.1.0",
  "description": "Opinionated, complete library for GraphQL + TypeScript development",
  "keywords": [
    "graphql",
    "typescript",
    "code-generation",
    "server",
    "client",
    "websockets"
  ],
  "contributors": [
    "Morris Brodersen <mb@morrisbrodersen.de> (https://morrisbrodersen.de)"
  ],
  "license": "ISC",
  "module": "src/index.ts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "example": "sh scripts/example.sh",
    "build": "sh scripts/build.sh",
    "build-examples": "sh scripts/build-examples.sh",
    "lint": "eslint .",
    "format": "prettier --write './**/*.ts' './**/*.js' './**/*.json'",
    "test": "nyc mocha test",
    "clean": "rm -rf .nyc_output build coverage dist",
    "prepare": "npm run clean && npm run build"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "src"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "html"
    ],
    "cache": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm test && npm run build"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint",
      "git add"
    ]
  },
  "peerDependencies": {
    "graphql": "^15"
  },
  "optionalDependencies": {
    "prettier": "^2",
    "typescript": "^4",
    "ws": "^7"
  },
  "devDependencies": {
    "@types/cors": "^2.8.9",
    "@types/express": "^4",
    "@types/got": "^9",
    "@types/graphql-iso-date": "^3",
    "@types/mocha": "^8",
    "@types/node": "^14.14.19",
    "@types/prettier": "^2.1.6",
    "@types/ws": "^7.4.0",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "eslint": "^7.15.0",
    "express": "^4",
    "got": "^11.8.1",
    "graphql": "^15",
    "graphql-iso-date": "^3.6.1",
    "husky": "^4",
    "lint-staged": "^10",
    "mocha": "^8",
    "nyc": "^15",
    "prettier": "^2",
    "source-map-support": "^0.5",
    "terser": "^5.5.1",
    "ts-node": "^9.1.1",
    "typescript": "^4",
    "ws": "^7.4.1"
  }
}
